<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/table_listitem"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/black"
    android:descendantFocusability="blocksDescendants" >

    <TextView
        android:id="@+id/name_p_setting"
        android:layout_width="0dip"
        android:layout_height="52dp"
        android:layout_gravity="left|center_vertical"
        android:layout_margin="1dp"
        android:layout_weight="1"
        android:background="@drawable/gray"
        android:gravity="center"
        android:focusable="false" 
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/foot_p_setting"
        android:layout_width="0dip"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_margin="1dp"
        android:layout_weight="1"
        android:background="@drawable/white"
        android:focusable="false" 
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <TextView
        android:id="@+id/productBA_p_setting"
        android:layout_width="0dip"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_margin="1dp"
        android:layout_weight="1"
        android:background="@drawable/white"
        android:focusable="false" 
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <TextView
        android:id="@+id/productUD_p_setting"
        android:layout_width="0dip"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_margin="1dp"
        android:layout_weight="1"
        android:background="@drawable/white"
        android:focusable="false"  
        android:textAppearance="?android:attr/textAppearanceMedium"/>
    <TextView
        android:id="@+id/feedertroughBA_p_setting"
        android:layout_width="0dip"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_margin="1dp"
        android:layout_weight="1"
        android:background="@drawable/white"
        android:focusable="false"  
        android:textAppearance="?android:attr/textAppearanceMedium"/>
    <TextView
        android:id="@+id/feedertroughUD_p_setting"
        android:layout_width="0dip"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_margin="1dp"
        android:layout_weight="1"
        android:background="@drawable/white"
        android:focusable="false" 
        android:textAppearance="?android:attr/textAppearanceMedium" />

</LinearLayout>